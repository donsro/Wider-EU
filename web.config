<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpErrors>
            <remove statusCode="502" subStatusCode="-1" />
            <remove statusCode="501" subStatusCode="-1" />
            <remove statusCode="500" subStatusCode="-1" />
            <remove statusCode="412" subStatusCode="-1" />
            <remove statusCode="406" subStatusCode="-1" />
            <remove statusCode="405" subStatusCode="-1" />
            <remove statusCode="404" subStatusCode="-1" />
            <remove statusCode="401" subStatusCode="-1" />
            <error statusCode="401" path="/c-error/401.htm" responseMode="ExecuteURL" />
            <error statusCode="404" path="/c-error/404.htm" responseMode="ExecuteURL" />
            <error statusCode="405" path="/c-error/405.htm" responseMode="ExecuteURL" />
            <error statusCode="406" path="/c-error/406.htm" responseMode="ExecuteURL" />
            <error statusCode="412" path="/c-error/412.htm" responseMode="ExecuteURL" />
            <error statusCode="500" path="/c-error/500.htm" responseMode="ExecuteURL" />
            <error statusCode="501" path="/c-error/501.htm" responseMode="ExecuteURL" />
            <error statusCode="502" path="/c-error/502.htm" responseMode="ExecuteURL" />
        </httpErrors>
    </system.webServer>
</configuration>
